script/source = "
extends CharacterBody2D

@onready var body = $Body
@onready var collision = $CollisionShape2D
@onready var upgrades_menu = $GUI/Upgrades

# Attacks
@onready var gun = $Attack/Gun
@onready var bullet = preload('res://source/object/bullet.tscn')
@onready var bullet_timer = $Attack/BulletTimer
@onready var bullet_attack_timer = $Attack/BulletTimer/BulletAttackTimer
var bullet_level = 1
var bullet_attack_speed = 0.1 # 0.1

var hp = 100

var money = 0
@onready var money_label = $GUI/InfoBar/Money

var enemy_close = []

var level_switch = false
@onready var level = $GUI/Level
@onready var level_animation = $GUI/Level/AnimationPlayer

# 进程函数 ==============================================================

func _ready():
	upgrades_menu.visible = false
	gun.rotation = 44
	attack()
	
func _process(delta):
		
	if Input.is_action_just_pressed(\"ui_up\"):
		$Body/HitBox.collision.disabled = false
		
	if Input.is_action_just_released(\"ui_up\"):
		$Body/HitBox.collision.disabled = true
		
	#body_rotation += car_ang_speed * body_direction # * delta
		
	if Input.is_action_just_pressed('ui_up') or Input.is_action_just_pressed('ui_down'):
		$AudioStreamPlayer.play()
		
	if Input.is_action_just_released('ui_up') or Input.is_action_just_pressed('ui_down'):
		$AudioStreamPlayer.stop()
		
	# if Input.is_action_pressed('esc'):
	# 	# get_tree().change_scene_to_packed(menu_main)
	# 	SceneTransition.change_scene('res://source/scene/menu_main.tscn')
		
func _physics_process(delta):
	if hp <= 0:
		# get_tree().change_scene_to_file('res://source/scene/menu_gameover.tscn')
		SceneTransition.change_scene('res://source/scene/menu_gameover.tscn')

	$HUD/HP.value = hp
	money_label.text = str(money)
	
	move_and_slide()

# 自定义函数 =============================================================
		
func attack():
	if bullet_level == 0:
		$Attack/Gun.hide()
	if bullet_level > 0:
		$Attack/Gun.show()
		bullet_timer.wait_time = bullet_attack_speed
		if bullet_timer.is_stopped():
			bullet_timer.start()
			
# 信号 ================================================================

# 受伤箱信号
func _on_hurt_box_hurt(damage, _angle, _knock_back):
	hp -= damage
	
# 子弹信号
func _on_bullet_timer_timeout():
	if len(enemy_close) > 1:
		var bullet_attack = bullet.instantiate()
		bullet_attack.target = get_random_target()
		$Attack/Gun.look_at(bullet_attack.target)
		bullet_attack.level = bullet_level
		add_child(bullet_attack)
			
func get_random_target():
	if enemy_close.size() > 0:
		return enemy_close.pick_random().global_position
	else:
		return Vector2.UP

# 敌人检测区域信号
func _on_enemy_detection_area_body_entered(body):
	if not enemy_close.has(body):
		enemy_close.append(body)

func _on_enemy_detection_area_body_exited(body):
	if enemy_close.has(body):
		enemy_close.erase(body)

func _on_grab_area_area_entered(area):
	if area.is_in_group('loot'):
		area.target = self

func _on_collect_area_area_entered(area):
	if area.is_in_group('loot'):
		money += area.collect()
	if area.is_in_group('shop'):
		get_tree().paused = true
		upgrades_menu.visible = true
	if area.is_in_group('next_part'):
		level_switch = true
		level.text = str(int(level.text)+1)

# 引擎升级
func _on_engine_power_slider_value_changed(value):
	body.engine_power = value

# 转向机升级
func _on_steering_angle_slider_value_changed(value):
	body.steering_angle = value
	
# 刹车升级
func _on_breaking_slider_value_changed(value):
	body.breaking = value
	
# 轮胎抓地力升级
func _on_wheel_friction_slider_value_changed(value):
	body.slip_speed = value

# 武器升级
func _on_weapon_up_pressed():
	if money >= 100:
		if bullet_level < 5:
			bullet_level += 1
			bullet_attack_speed -= 0.01
			$GUI/Upgrades/Weapon/Speed/HBoxContainer/Value.text = str(bullet_level)
			money -= 100
			money_label.text = str(money)






"

